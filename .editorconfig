# top-most EditorConfig file
root = true

# UTF-8 Charset, Unix-style newlines with a newline ending every file and no trailing whitespace
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false
